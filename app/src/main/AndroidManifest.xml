<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.gs1.barcodescanner">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".addNewLink"></activity>
        <activity
            android:name=".editLinkPage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".addProductPage1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".addProductPage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".landingPage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".grabBrowserUrl"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".product_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".loginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".mybrowser"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ScanCodeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>