<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.gs1.barcodescanner">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".add_new_link_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".edit_link_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".edit_product_info_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".add_new_product_page2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".add_new_product_page1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".consumer_landing_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".grab_browser_url"/>
        <activity
            android:name=".product_link_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".login_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".in_app_browser"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".barcode_scanner_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".main_page"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>